    <Assign DisplayName="최근 메일 변수 생성 | Assign" sap:VirtualizedContainerService.HintSize="479,122" sap2010:WorkflowViewState.IdRef="Assign_1">
      <Assign.To>
        <OutArgument x:TypeArguments="x:String">[MailContents]</OutArgument>
